{
    "version": 2,
    "buildCommand": "",
    "outputDirectory": ".",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/app",
            "dest": "/app.html"
        },
        {
            "src": "/app.html",
            "dest": "/app.html"
        },
        {
            "src": "/(.*\\.js)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.css)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.png)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.ico)",
            "dest": "/$1"
        },
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "GEMINI_API_KEY": "@gemini_api_key"
    }
}